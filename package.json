{
  "name": "logger-with-context",
  "description": "Logger with context",
  "version": "0.1.2-snapshot.4",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start:dev": "ts-node src/index.ts",
    "test": "jest --testMatch='**/test/**/*.test.ts'",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ./src/**/*.ts ./test/**/*.ts",
    "build": "tsup",
    "audit:fix": "npm audit fix",
    "audit:check": "npx audit-ci --config ./audit-ci.jsonc",
    "dep:check": "npx --no-install ncu -e 2",
    "dep:update": "npx --no-install ncu -u",
    "snapshot": "npx --no -- standard-version --no-verify --skip.changelog --prerelease snapshot --releaseCommitMessageFormat 'chore(snapshot): {{currentTag}}'",
    "prepare": "husky || true"
  },
  "devDependencies": {
    "@commitlint/cli": "20.0.0",
    "@commitlint/config-conventional": "20.0.0",
    "audit-ci": "7.1.0",
    "@types/jest": "30.0.0",
    "@types/node": "24.5.2",
    "@typescript-eslint/eslint-plugin": "8.44.1",
    "@typescript-eslint/parser": "8.44.1",
    "eslint": "9.36.0",
    "eslint-config-prettier": "10.1.8",
    "husky": "9.1.7",
    "jest": "30.1.3",
    "lint-staged": "16.2.1",
    "npm-check-updates": "18.3.0",
    "prettier": "3.6.2",
    "ts-jest": "29.4.4",
    "ts-node": "10.9.2",
    "tsup": "8.5.0",
    "typescript": "5.9.2"
  },
  "lint-staged": {
    "./src/*.ts": [
      "eslint . --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "keywords": [
    "logger",
    "logging",
    "context"
  ],
  "author": "geka.evk@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/geka-evk/context-logger.git"
  },
  "engines": {
    "node": ">=20"
  }
}
